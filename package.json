  "scripts": {
    "clean": "rimraf ./dist",
    "rb": "rollup -c",
    "build": "rimraf ./dist && rollup -c",
    "watch": "npx tsc --watch",
    "build-publish": "rimraf ./dist && npm run build && npm publish",
    "prepare": "husky install",
    "codegen": "apollo client:codegen --endpoint=xxx --target=typescript --outputFlat src/graphql/types/autoGeneratedGraphql --includes=src/graphql/**/**/*.*",
    "postversion": "git push --follow-tags",
    "download_schema": "apollo client:download-schema --endpoint=xxx schema/schema.json"
  },
